<!DOCTYPE html>
<html lang="en">
<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/w3css.css">
    <%- include('../partials/dashboard/head.ejs') %>
    <style>
     
    </style>
</head>
<body>
    <%- include('../partials/dashboard/sidebar.ejs') %>
    <section class="vastmint-dashboard-page">
    <%- include('../partials/header2.ejs') %>
    <%- include('../partials/dashboard/header.ejs') %>
       <p class="fs-4 fw-semi-bold">Your Minted Art</p>
       <div class="vastmint-grid">
        <% for( let index = 0; index < 8; index++ ) { %>
            <div class="w3-white w3-card grid-col-6 grid-col-md-6 grid-col-sm-12 w3-round-large me-2 " style="min-height:230px">
                <div class="cover-img" style="height:250px;">
                    <img style="height:250px;border-radius:8px 8px 0 0" class="img-responsive object-fit-cover" src="/assets/nft-<%= index + 1 %>.jpg" alt="">
                </div>
                <div class="d-flex justify-content-between">
                    <div class="p-2">
                        <p class="fw-normal mb-0">Jane Doe</p>
                        <p class="vastmint-text-light-400">Art <%= index + 1%></p>
                     </div>
                     <div class="p-2">
                        <p class="vastmint-text-light-700 m-0"><small>Price</small></p>
                        <p class=""><%= (Math.random() * 3).toFixed(1) %> ETH</p>
                     </div>
                </div>
              </div>
        <% } %>
       </div>
    </section>
</body>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
</html>
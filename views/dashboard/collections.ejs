<!DOCTYPE html>
<html lang="en">
<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/w3css.css">
    <%- include('../partials/dashboard/head.ejs') %>
    <style>
     
    </style>
</head>
<body>
    <%- include('../partials/dashboard/sidebar.ejs') %>
    <section class="vastmint-dashboard-page">
    <%- include('../partials/header2.ejs') %>
    <%- include('../partials/dashboard/header.ejs') %>
       <p class="fs-4 fw-semi-bold">Your Collections</p>
       <div class="vastmint-grid">
        <% for( let index = 0; index < 8; index++ ) { %>
            <div class="w3-white grid-col-4 grid-col-md-6 grid-col-sm-12 w3-round-large me-2 " style="min-height:230px">
                <div class="cover-img" style="height:200px;">
                    <img style="height:200px;border-radius:8px 8px 0 0" class="img-responsive object-fit-cover" src="/assets/nft-<%= index + 1 %>.jpg" alt="">
                </div>
                <div class="p-2 text-center">
                   <p class="fw-bold mb-0">Collection <%= index + 1%></p>
                   <p class="vastmint-text-light-400"><%= Math.floor(Math.random() * 5) + 1 %> Artwork(s)</p>
                </div>
              </div>
        <% } %>
       </div>
    </section>
</body>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
</html>
<!DOCTYPE html>
<html lang="en">
<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-GLhlTQ8iRABdZLl6O3oVMWSktQOp6b7In1Zl3/Jr59b6EGGoI1aFkw7cmDA6j6gD" crossorigin="anonymous">
    <link rel="stylesheet" href="/css/w3css.css">
    <%- include('../partials/dashboard/head.ejs') %>
    <style>
     
    </style>
</head>
<body>
    <%- include('../partials/dashboard/sidebar.ejs') %>
    <section class="vastmint-dashboard-page">
    <%- include('../partials/header2.ejs') %>
    <%- include('../partials/dashboard/header.ejs') %>
       <p class="fs-4 fw-semi-bold">Wallet Details</p>
       <div class="vastmint-grid">
          <div class="grid-col-6 grid-col-md-12">
            <div class="p-3 w3-white w3-round-large w3-card">
                <header class="d-flex align-items-center justify-content-end">
                   <div>
                   <button class="w3-button w3-round-large py-1 px-2 w3-hover-light-grey w3-light-grey"><i class="bi bi-arrow-clockwise"></i></button>
                   </div>
                </header>
                <div class="d-flex align-items-center justify-content-between mt-3">
                   <div>
                       <p class="fs-5 fw-bold m-0">My Balance</p>
                       <p class="fw-bold fs-3 vastmint-text-light-600">12.2ETH</p>
                   </div>
                  <select name="" class="border-0 p-2 w3-light-grey w3-round-large" id="">
                     <option value="ETH">ETH</option>
                     <option value="SOL">SOL</option>
                     <option value="USDT">USDT</option>
                     <option value="USD">USD</option>
                  </select>
                </div>
                </div>
          </div>
       </div>
       <p class="mt-4 fs-3">Transaction History</p>
       <div class="mt-1 w3-responsive">
        <table class="w3-table w3-white w3-bordered w3-hoverable  w3-card w3-round-large">
            <tr>
              <th>Type</th>
              <th>Amount</th>
              <th>Status</th>
              <th>Date</th>
            </tr>
            <tr class="" style="cursor:pointer">
              <td>Minting Fee</td>
              <td>0.3ETH</td>
              <td>Credit</td>
              <td><%= new Date().toUTCString() %></td>
              <!-- <td>50</td> -->
            </tr>
            </table>
       </div>
    </section>
</body>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js" integrity="sha384-w76AqPfDkMBDXo30jS1Sgez6pr3x5MlQ1ZAGC+nuZB+EYdgRZgiwxhTBTkF7CXvN" crossorigin="anonymous"></script>
</html>
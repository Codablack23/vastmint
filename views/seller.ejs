<!DOCTYPE html>
<html>
<head>
<%- include("./partials/head.ejs") %>
</head>
<body>
    <%- include('./partials/home/nav.ejs') %>
    <main>
      <section class="vastmint-marketplace-page">
        <br>
        <section class="marketplace-main">
          <% if (response.status == "success") { %>
            <%- include('./partials/sellers/seller_banner.ejs') %>
            <%- include('./partials/sellers/seller-collections.ejs') %>
            <%- include('./partials/sellers/nfts.ejs') %>
          <% }else{ %>
            <br><br><br>
            <div class="" style="text-align:center">
                <% if (response.status_code == 500) { %>
                  <img src="/assets/500.svg" alt="page-not-found" style="max-width:380px;margin-bottom:0.5em">
                <% }else{ %>
                  <img src="/assets/404.svg" alt="page-not-found" style="max-width:380px;margin-bottom:0.5em">
                <% } %>
                <h3 style="font-size: 28px;"><%= response.status_code == 404?"Not Found":"Server Error"%></h3>
                <p><%= response.error %></p><br>
                <a href="/" class="vastmint-primary-400 vastmint-text-white" style="padding:10px 16px;border-radius:30px">Return Home</a>
            </div>
          <% } %>
           
        </section>
      </section>
    </main>
    
</body>
</html>